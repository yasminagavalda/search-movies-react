{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier", "compat", "react"],
  "env": {
    "node": true,
    "mocha": true,
    "browser": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "class-methods-use-this": 0,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "compat/compat": "error",
    "react/jsx-uses-vars": [2]
  },
  "parser": "babel-eslint"
}
